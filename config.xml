<?xml version='1.0' encoding='utf-8'?>
<widget id="com.solsort.bibapp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>BibApp</name>
    <description>Biblioteks inspirations-app</description>
    <author email="app@solsort.com" href="http://solsort.com">solsort.com</author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <icon src="www/bibapp-icon.png" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="Orientation" value="portrait" />
    <platform name="android">
    </platform>
    <platform name="ios">
    </platform>
    <engine name="android" spec="~4.1.1" />
</widget>
